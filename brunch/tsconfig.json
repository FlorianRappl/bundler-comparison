{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es5",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "jsx": "react",
    "baseUrl": "."
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules/**/*"
  ]
}
